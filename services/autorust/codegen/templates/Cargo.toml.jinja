# generated by AutoRust
[package]
name = "{{package_name}}"
version = "0.6.0"
edition = "2021"
license = "MIT"
description = "generated REST API bindings"
repository = "https://github.com/azure/azure-sdk-for-rust"
homepage = "https://github.com/azure/azure-sdk-for-rust"
documentation = "https://docs.rs/{{package_name}}"

[lib]
doctest = false

[dependencies]
azure_core = { path = "../../../sdk/core", version = "0.5", features = [{%- for feature in azure_core_features -%}"{{feature}}", {%- endfor -%}] }
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
bytes = "1.0"
futures = "0.3"
time = "0.3"

[dev-dependencies]
azure_identity = { path = "../../../sdk/identity" }
tokio = { version = "1.0", features = ["macros", "rt-multi-thread"] }
env_logger = "0.9"

[package.metadata.docs.rs]
all-features = true

[features]
default = ["{{default_feature}}", "enable_reqwest"]
enable_reqwest = ["azure_core/enable_reqwest"]
enable_reqwest_rustls = ["azure_core/enable_reqwest_rustls"]
no-default-tag = []
{%- for feature in features %}
"{{feature}}" = []
{%- endfor %}
