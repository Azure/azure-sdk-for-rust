{
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "ignorePaths": [
    "**/test-resources.bicep",
    "**/test-resources.json",
    "**/test-resources-post.ps1",
    "**/assets.json",
    "**/*.pfx",
    ".config",
    ".devcontainer/devcontainer.json",
    ".devcontainer/Dockerfile",
    ".devcontainer/oncreate",
    ".github/",
    ".vscode/cspell.json",
    ".vscode/extensions.json",
    ".vscode/settings.json",
    ".vscode/tasks.json",
    "NOTICE.txt",
    "deny.toml",
    "eng/",
    "rust-toolchain.toml"
  ],
  "words": [
    "aarch",
    "accountendpoint",
    "accountkey",
    "amqp",
    "asyncoperation",
    "azsdk",
    "azurecli",
    "bugbug",
    "checkpointstore",
    "clippy",
    "codeowners",
    "contoso",
    "cplusplus",
    "cpptools",
    "cppvsdbg",
    "datalake",
    "datetime",
    "deserializers",
    "devicecode",
    "docsrs",
    "doctest",
    "dotenv",
    "downcasted",
    "downcasting",
    "entra",
    "etag",
    "eventhub",
    "eventhubs",
    "execdir",
    "headerless",
    "hmac",
    "iothub",
    "keyvault",
    "lastexitcode",
    "linuxpool",
    "linuxvmimage",
    "lldb",
    "macpool",
    "macvmimage",
    "markdownlintrc",
    "maxresults",
    "maxsize",
    "msrc",
    "msrv",
    "nocapture",
    "newtonsoft",
    "oidc",
    "opentelemetry",
    "otel",
    "pageable",
    "pageables",
    "pkce",
    "pkcs",
    "posix",
    "pullrequest",
    "pwsh",
    "reqwest",
    "resourcemanager",
    "rngs",
    "rsplit",
    "rustfmt",
    "runtimes",
    "rustfmt",
    "rustup",
    "schannel",
    "seekable",
    "servicebus",
    "spector",
    "stylesheet",
    "subclient",
    "telemetered",
    "typespec",
    "undelete",
    "uninlined",
    "upvote",
    "userdelegationkey",
    "vcpkg",
    "versionid",
    "virtualmachine",
    "windowspool",
    "windowsvmimage",
    "webpki",
    "worktree"
  ],
  "dictionaryDefinitions": [
    {
      "name": "crates",
      "path": "../eng/dict/crates.txt",
      "noSuggest": true,
      "useCompounds": false
    },
    {
      "name": "rust-custom",
      "path": "../eng/dict/rust-custom.txt",
      "noSuggest": true
    }
  ],
  "overrides": [
    {
      "filename": "**/Cargo.toml",
      "dictionaries": [
        "crates",
        "rust-custom"
      ]
    },
    {
      "filename": "**/*.md",
      "dictionaries": [
        "crates",
        "rust",
        "rust-custom"
      ],
      "ignoreRegExpList": [
        "@\\w+"
      ]
    },
    {
      "filename": "**/*.rs",
      "dictionaries": [
        "crates",
        "rust-custom"
      ]
    },
    {
      "filename": "sdk/core/typespec*/**",
      "flagWords": [
        "azure",
        "azurite"
      ]
    }
  ]
}