{
  "name": "@azure-typespec/http-client-rust",
  "version": "0.1.0",
  "description": "TypeSpec emitter for Rust client SDKs",
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js"
  },
  "tspMain": "dist/src/index.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "cadl-ranch": "node .scripts/cadl-ranch.js",
    "clean": "ver > nul 2>&1 nul && npm run --silent clean-cmd || npm run --silent clean-bash",
    "clean-cmd": "if exist dist rmdir /s /q dist && exit 0 || exit 0 ",
    "clean-bash": "rm nul && rm -rf dist && exit 0 || exit 0",
    "build": "tsc -p .",
    "eslint-fix": "eslint . --fix --ext .ts",
    "eslint": "eslint . --ext .ts",
    "test": "vitest run",
    "test-ci": "vitest run --coverage --reporter=junit --reporter=default --no-file-parallelism",
    "tspcompile": "node .scripts/tspcompile.js",
    "watch": "tsc -p . --watch"
  },
  "files": [
    "dist/src/**",
    "!dist/test/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/azure-sdk-for-rust.git"
  },
  "keywords": [
    "cadl",
    "rust",
    "rustlang",
    "typespec"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/azure-sdk-for-rust/issues"
  },
  "devDependencies": {
    "@azure-tools/cadl-ranch": "~0.14.6",
    "@azure-tools/cadl-ranch-expect": "~0.15.4",
    "@azure-tools/cadl-ranch-specs": "0.37.4",
    "@azure-tools/typespec-autorest": "0.45.0",
    "@azure-tools/typespec-azure-resource-manager": "0.45.0",
    "@types/js-yaml": "~4.0.9",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "~4.1.1",
    "@typescript-eslint/parser": "~4.1.1",
    "@typespec/openapi": "~0.59.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "c8": "^9.1.0",
    "eslint": "~6.6.0",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@azure-tools/codegen": "~2.9.2",
    "@azure-tools/linq": "~3.1.263",
    "@azure-tools/typespec-azure-core": "^0.46.0",
    "@azure-tools/typespec-client-generator-core": "0.46.2",
    "@typespec/compiler": "^0.60.1",
    "@typespec/http": "^0.60.0",
    "@typespec/rest": "^0.60.0",
    "@typespec/versioning": "^0.60.1",
    "@typespec/xml": "^0.60.0",
    "js-yaml": "~4.1.0",
    "source-map-support": "0.5.21",
    "vitest": "^1.6.0"
  }
}
