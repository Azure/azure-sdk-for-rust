{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": false,
    "lib": ["es2019"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "newLine": "LF",
    "noEmitHelpers": false,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "outDir": "dist",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "target": "es2019",
    "types": ["node"],
    "useUnknownInCatchVariables": false
  },
  "include": ["vitest.config.ts", "src/**/*", "test/**/*.ts"],
  "exclude": ["dist", "resources", "node_modules", "**/*.d.ts", "generated"]
}
